{
    "name": "modulabs/laravel-google-merchant",
    "description": "Laravel Google Merchant Products API for Google Shopping.",
    "homepage": "https://github.com/guillmz/laravel-google-merchant",
    "version": "1.0.0",
    
    "license": "MIT",
    "authors": [
        {
            "name": "FGuillen",
            "email": "info@modulabs.dev",
            "role": "Developer"
        }
    ],
    "keywords":[
        "Laravel",
        "Google Merchant API",
        "Google Shopping API"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/cache": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle":"~5.3|^6.5|^7.0",
        "google/apiclient": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "GUILLMZ\\GoogleMerchantApi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GUILLMZ\\GoogleMerchantApi\\GoogleMerchantApiServiceProvider"
            ],
            "aliases": {
                "ProductsApi": "GUILLMZ\\GoogleMerchantApi\\Facades\\ProductsApi"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/guillmz/laravel-google-merchant"
        }
    ],
    "minimum-stability": "stable",
}
